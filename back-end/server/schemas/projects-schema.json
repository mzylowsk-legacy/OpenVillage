{
  "addNewProject": {
    "id": "AddNewProject",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9][a-zA-Z0-9-_\\.]+$"
      },
      "password": {
        "type": "string"
      },
      "isPrivate": {
        "type":  { "enum": [ false, true ] }
      },
      "description": {
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "name": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9][a-zA-Z0-9-_\\.]+$"
      },
      "creationDate": {
        "type": "datetime"
      }
    },
    "required": [
      "isPrivate",
      "description",
      "url",
      "name"
    ],
    "minProperties": 4,
    "maxProperties": 7
  }
}
